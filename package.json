{
  "name": "elysia-app",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/benchmark-server.ts",
    "dev-db": "docker compose -f ./docker-compose.yml up -d && trap 'docker compose down' INT TERM EXIT && bun run --watch src/benchmark-server.ts",
    "build": "bun build src/benchmark-server.ts --outdir dist --target bun --minify",
    "build-tsx": "bun x tsc",
    "seed": "bun src/scripts/seed-fake-users.ts",
    "start": "bun dist/benchmark-server.js",
    "start-db": "docker compose -f ./docker-compose.yml up -d && trap 'docker compose down' INT TERM EXIT &&bun dist/benchmark-server.js",
    "db-up": "docker compose -f ./docker-compose.yml up -d",
    "db-down": "docker compose down",
    "deploy": "bun run build-tsx && vercel --prod"
  },
  "dependencies": {
    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/opentelemetry": "^1.4.9",
    "@elysiajs/swagger": "^1.3.1",
    "@faker-js/faker": "^10.1.0",
    "@types/pg": "^8.16.0",
    "drizzle-orm": "^0.45.0",
    "elysia": "^1.4.18",
    "pg": "^8.16.3",
    "pg-pool": "^3.10.1",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "bun-types": "latest",
    "drizzle-kit": "^0.31.8",
    "typescript": "^5.9.3"
  },
  "type": "module"
}
